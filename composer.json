{
  "name": "seinoxygen/laravel-contabilium",
  "description": "A Laravel wrapper for Contabilium",
  "keywords": [
    "laravel",
    "contabilium"
  ],
  "license": "MIT",
  "authors": [
    {
      "company": "",
      "email": "seinoxygen@hotmail.com",
      "url": ""
    }
  ],
  "support": {
    "email": "seinoxygen@hotmail.com"
